<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/abouut"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="54dp"
     android:background="@color/md_white_1000"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/TextView01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:fontFamily="sans-serif-light"
        android:text="Chiheb Manai"
        android:textColor="#2196f3"
        android:textSize="@dimen/textsize1"
        android:textStyle="bold"
        android:layout_alignTop="@+id/imageView2"
        android:layout_toEndOf="@+id/imageView2" />


    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:fontFamily="sans-serif-light"
        android:text="ss"
        android:textAlignment="gravity"
        android:textSize="@dimen/textsize2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView1"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginTop="17dp"
        android:fontFamily="sans-serif-light"
        android:text="Developed By"
        android:textColor="#2196f3"
        android:textSize="@dimen/textsize3"
        android:textStyle="bold" />

      <tn.celestialsoftware.iwillcometotunisia.RoundedImageView
        android:id="@+id/imageView2"
        android:layout_width="@dimen/width3"
        android:layout_height="@dimen/height1"
        android:layout_alignLeft="@+id/textView2"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="@dimen/margintop2"
        android:scaleType="centerCrop"
        android:src="@drawable/chi" />
    <tn.celestialsoftware.iwillcometotunisia.RoundedImageView
        android:id="@+id/imageView4"
        android:layout_width="@dimen/width3"
        android:layout_height="@dimen/height1"
        android:scaleType="centerCrop"
        android:src="@drawable/pira"
        android:layout_below="@+id/imageView3"
        android:layout_alignStart="@+id/imageView3"
        android:layout_marginTop="17dp" />
    <tn.celestialsoftware.iwillcometotunisia.RoundedImageView
        android:id="@+id/imageView5"
        android:layout_width="@dimen/width3"
        android:layout_height="@dimen/height1"
        android:scaleType="centerCrop"
        android:src="@drawable/commando"
        android:layout_below="@+id/imageView6"
        android:layout_alignStart="@+id/imageView6"
        android:layout_marginTop="17dp" />
    <tn.celestialsoftware.iwillcometotunisia.RoundedImageView
        android:id="@+id/imageView6"
        android:layout_width="@dimen/width3"
        android:layout_height="@dimen/height1"
        android:scaleType="centerCrop"
        android:src="@drawable/mohamed"
        android:layout_below="@+id/imageView4"
        android:layout_alignStart="@+id/imageView4"
        android:layout_marginTop="17dp" />


    <TextView
        android:id="@+id/TextView02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:text="Wajdi Mnasri"
        android:textColor="#2196f3"
        android:textSize="@dimen/textsize1"
        android:textStyle="bold"
        android:layout_alignTop="@+id/imageView3"
        android:layout_alignStart="@+id/TextView01" />

    <tn.celestialsoftware.iwillcometotunisia.RoundedImageView
        android:id="@+id/imageView3"
        android:layout_width="@dimen/width3"
        android:layout_height="@dimen/height1"
        android:scaleType="centerCrop"
        android:src="@drawable/ja"
        android:layout_below="@+id/imageView2"
        android:layout_alignStart="@+id/imageView2"
        android:layout_marginTop="17dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:text="Aymen Talbi"
        android:textColor="#2196f3"
        android:textSize="@dimen/textsize1"
        android:textStyle="bold"
        android:layout_alignTop="@+id/imageView4"
        android:layout_alignStart="@+id/TextView02" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:text="Mohamed Sallah"
        android:textColor="#2196f3"
        android:textSize="@dimen/textsize1"
        android:textStyle="bold"
        android:layout_alignTop="@+id/imageView6"
        android:layout_alignStart="@+id/textView" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:text="Ahmed Bejaoui"
        android:textColor="#2196f3"
        android:textSize="@dimen/textsize1"
        android:textStyle="bold"
        android:layout_alignTop="@+id/imageView5"
        android:layout_alignStart="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="chiheb.manai@esprit.tn"
        android:id="@+id/textView5"
        android:textColor="#ff171717"
        android:textSize="15sp"
        android:layout_below="@+id/TextView01"
        android:layout_alignStart="@+id/TextView01" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="wajdi.mnasri@esprit.tn"
        android:id="@+id/textView6"
        android:textColor="#ff171717"
        android:textSize="15sp"
        android:layout_below="@+id/TextView02"
        android:layout_alignStart="@+id/TextView02" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="aymen.talbi@esprit.tn"
        android:id="@+id/textView7"
        android:textColor="#ff171717"
        android:textSize="15sp"
        android:layout_below="@+id/textView"
        android:layout_alignStart="@+id/textView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="mohamed.sallah@esprit.tn"
        android:id="@+id/textView8"
        android:textColor="#ff171717"
        android:textSize="15sp"
        android:layout_below="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="ahmed.bejaoui@esprit.tn"
        android:id="@+id/textView9"
        android:textColor="#ff171717"
        android:textSize="15sp"
        android:layout_below="@+id/textView4"
        android:layout_alignStart="@+id/textView4" />


</RelativeLayout>